<p id="notice"><%= notice %></p>

<div id="entire">
  <div id="title">
    <h1><%= @event.name %></h1>
  </div>
  <div id="info">
    <div id="text">
      <h2>Date</h2>
      <p><%= @event.date %></p>
      <h2>Region</h2>
      <p><%= @event.region %></p>
      <h2>Spots Left</h2>
      <p><%= @event.spotsLeft %> / <%= @event.spotsAvailable %></p>
      
      <h2>Who's Going!</h2>
      <ul>
        <%= render(@event.event_attendees) %>
      </ul>
    </div>
    <div id="map">
      <!-- PUT MAP HERE -->
    </div>
  </div>
</div>